BEGIN MAIN_PROGRAM_NAME
        DEFINE INTEGER stop SET 5
        WRITE "\n" SUM summation ROUND_OPEN stop ROUND_CLOSE 
    END MAIN_PROGRAM_NAME

    FUNCTION INTEGER summation ROUND_OPEN INTEGER n ROUND_CLOSE
        DEFINE INTEGER sum SET 0
        ITERATE INTEGER i FROM 0  TO n  STEP 1 
            WRITE i SUM " " 
            EXECUTE sum SET sum SUM i
        END ITERATE
        RETURN sum 
    END summation