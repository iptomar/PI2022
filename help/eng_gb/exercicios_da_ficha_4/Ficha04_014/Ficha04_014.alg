    inicio 
        escrever "Truth table of OR\n"
        logico var = verdadeiro || verdadeiro
        escrever "\n" + verdadeiro + "\t||\t" + verdadeiro + "\t= " + var
        var = verdadeiro || falso
        escrever "\n" + verdadeiro + "\t||\t" + falso + "\t\t= " + var
        var = falso || verdadeiro
        escrever "\n" + falso + "\t\t||\t" + verdadeiro + "\t= " + var
        escrever "\n" + falso + "\t\t||\t" + falso + "\t\t= " + (falso || falso)
    fim 


